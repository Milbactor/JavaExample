<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://www.netbeans.org/ns/project/1">
    <type>org.netbeans.modules.ant.freeform</type>
    <configuration>
        <general-data xmlns="http://www.netbeans.org/ns/freeform-project/1">
            <name>test</name>
        </general-data>
        <general-data xmlns="http://www.netbeans.org/ns/freeform-project/2">
            <!-- このファイルを手動で編集するときは、プロジェクト・プロパティのカスタマイザは使用しないでください。 -->
            <name>test</name>
            <properties>
                <property name="ant.script">main/Main.iml</property>
            </properties>
            <folders>
                <source-folder>
                    <label>main</label>
                    <type>java</type>
                    <location>main</location>
                    <encoding>windows-31j</encoding>
                </source-folder>
                <source-folder>
                    <label>test</label>
                    <type>java</type>
                    <location>test</location>
                    <encoding>windows-31j</encoding>
                </source-folder>
                <source-folder>
                    <label>test</label>
                    <location>.</location>
                    <encoding>windows-31j</encoding>
                </source-folder>
            </folders>
            <ide-actions/>
            <view>
                <items>
                    <source-folder style="packages">
                        <label>main</label>
                        <location>main</location>
                    </source-folder>
                    <source-folder style="packages">
                        <label>test</label>
                        <location>test</location>
                    </source-folder>
                    <source-file>
                        <location>${ant.script}</location>
                    </source-file>
                </items>
                <context-menu/>
            </view>
        </general-data>
        <java-data xmlns="http://www.netbeans.org/ns/freeform-project-java/2">
            <compilation-unit>
                <package-root>main</package-root>
                <source-level>1.5</source-level>
            </compilation-unit>
            <compilation-unit>
                <package-root>test</package-root>
                <unit-tests/>
                <source-level>1.5</source-level>
            </compilation-unit>
        </java-data>
    </configuration>
</project>
